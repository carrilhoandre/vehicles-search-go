# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Vehicle {
  id: ID!
  makeId: ID!
  makeName: String!
  modelName: String!
}

type Query {
  vehicles(text: String = ""): [Vehicle!]!
}

input NewQuery {
  text: String!
  id: ID!
}

type Mutation {
  createMake(input: NewQuery!): Vehicle!
}